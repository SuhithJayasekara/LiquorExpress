<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.liquorexpress">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_logo"
        android:label="@string/app_name_long"
        android:roundIcon="@drawable/ic_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.liquorexpress.FoodEditActivity"></activity>
        <activity android:name="com.example.liquorexpress.OrderItemActivity" />
        <activity android:name="com.example.liquorexpress.UserItemActivity" />
        <activity android:name="com.example.liquorexpress.CategoryItemActivity" />
        <activity android:name="com.example.liquorexpress.FoodItemActivity" />
        <activity android:name="com.example.liquorexpress.CategoryFoodActivity" />
        <activity android:name="com.example.liquorexpress.CartActivity" />
        <activity android:name="com.example.liquorexpress.LogInActivity" />
        <activity android:name="com.example.liquorexpress.SignUpActivity" />
        <activity android:name="com.example.liquorexpress.WelcomeActivity" />
        <activity
            android:name="com.example.liquorexpress.AdminMainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.liquorexpress.MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.example.liquorexpress.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>